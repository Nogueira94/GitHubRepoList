<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Github_api_app" parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">@color/darkgrey</item>
        <item name="colorPrimaryVariant">@color/midgrey</item>
        <item name="colorOnPrimary">@color/lightgrey</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>
    <style name="Widget.App.ButtonStyle"
        parent="Widget.MaterialComponents.Button">
        <item name="materialThemeOverlay">@style/WhiteButtonOverlay</item>
    </style>
    <style name="WhiteButtonOverlay">
        <item name="colorPrimary">@color/white2</item>
    </style>
</resources>